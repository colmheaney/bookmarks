<h1>Listing quote_items</h1>

<table>
  <thead>
    <tr>
      <th>Quote</th>
      <th>Product</th>
      <th>Cost</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_items.each do |quote_item| %>
      <tr>
        <td><%= quote_item.quote_id %></td>
        <td><%= quote_item.product_id %></td>
        <td><%= quote_item.cost %></td>
        <td><%= quote_item.quantity %></td>
        <td><%= link_to 'Show', quote_item %></td>
        <td><%= link_to 'Edit', edit_quote_item_path(quote_item) %></td>
        <td><%= link_to 'Destroy', quote_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote item', new_quote_item_path %>
